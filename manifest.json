{
    "name": "Replace with Cats",
    "description": "Replace all images on websites with images of cats",
    "version": "2.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "action": {
        "default_icon": {
            "16": "./icons/favicon.png",
            "32": "./icons/favicon.png",
            "48": "./icons/favicon.png",
            "128": "./icons/favicon.png"
        }
    },
    "icons": {
        "16": "./icons/favicon.png",
        "32": "./icons/favicon.png",
        "48": "./icons/favicon.png",
        "128": "./icons/favicon.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "/images/*.jpg"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}